{
  "name": "aroma-server",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test nyc --require @babel/register --require @babel/polyfill mocha test/**/*.js --timeout 5000 -w",
    "prebuild": "rm -rf dist",
    "build": "babel src --out-dir dist",
    "start": "node dist/bin/www.js",
    "prestart:local": "cp ./.env.local ./.env",
    "start:local": "NODE_ENV=development nodemon --exec babel-node src/bin/www.js --ignore 'test'",
    "start:dev": "NODE_ENV=development node dist/bin/www.js",
    "start:stage": "NODE_ENV=stage node dist/bin/www.js",
    "start:prod": "NODE_ENV=production node dist/bin/www.js"
  },
  "dependencies": {
    "aws-sdk": "2.747.0",
    "axios": "0.20.0",
    "bignumber.js": "9.0.0",
    "class-transformer": "0.3.1",
    "class-validator": "0.12.2",
    "cls-hooked": "4.2.2",
    "cls-rtracer": "2.3.0",
    "cookie-parser": "1.4.5",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "express-validator": "6.6.1",
    "fxjs": "0.16.3",
    "helmet": "4.1.0",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.20",
    "moment": "2.27.0",
    "moment-timezone": "0.5.31",
    "morgan": "1.10.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
