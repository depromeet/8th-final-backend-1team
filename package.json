    "eslint-import-resolver-babel-module": "5.1.2",
    "eslint-plugin-import": "2.22.0",
    "mocha": "8.1.3",
    "nock": "13.0.4",
    "nodemon": "2.0.4",
    "prettier": "2.1.1",
    "should": "13.2.3",
    "sinon": "9.0.3",
    "supertest": "4.0.2"
  }
